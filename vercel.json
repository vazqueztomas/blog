{
    "version":2,
    "builds":[
        {
            "src":"personal_blog/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambdaSize":"15mb",
                "runtime":"Python3.9"
            }
        },
        {
            "src":"build.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"static_files"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static_files/\\1"
        },
        {
            "src":"/(.*)",
            "dest":"personal_blog/wsgi.py"
        }
    ]
}